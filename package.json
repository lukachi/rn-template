{
  "name": "rn-template",
  "main": "src/main.ts",
  "version": "1.2.4",
  "scripts": {
    "prebuild": "npx expo prebuild --clean && npx pod-install",
    "start": "npx expo start --clear",
    "android": "npx expo run:android --device",
    "ios": "npx expo run:ios --device",
    "build:development:ios": "cross-env APP_ENV=development eas build --profile development --platform ios",
    "build:development:android": "cross-env APP_ENV=development eas build --profile development --platform android",
    "prebuild:staging": "cross-env APP_ENV=staging pnpm run prebuild",
    "start:staging": "cross-env APP_ENV=staging pnpm run start",
    "android:staging": "cross-env APP_ENV=staging pnpm run android",
    "ios:staging": "cross-env APP_ENV=staging pnpm run ios",
    "build:staging:ios": "cross-env APP_ENV=staging eas build --profile staging --platform ios",
    "build:staging:android": "cross-env APP_ENV=staging eas build --profile staging --platform android",
    "prebuild:production": "cross-env APP_ENV=production pnpm run prebuild",
    "start:production": "cross-env APP_ENV=production pnpm run start",
    "android:production": "cross-env APP_ENV=production pnpm run android",
    "ios:production": "cross-env APP_ENV=production pnpm run ios",
    "build:production:ios": "cross-env APP_ENV=production eas build --profile production --platform ios",
    "build:production:android": "cross-env APP_ENV=production eas build --profile production --platform android",
    "prepare-secrets": "node ./scripts/prepare-secrets.cjs",
    "release": "release-it --verbose",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "lint:fix": "eslint . --fix --cache --cache-location 'node_modules/.cache/.eslintcache' --max-warnings 0",
    "format": "prettier . --check --cache --cache-location='node_modules/.cache/.prettiercache'",
    "format:fix": "prettier . --write --cache --cache-location='node_modules/.cache/.prettiercache' --log-level=warn",
    "test": "jest --passWithNoTests",
    "test:ci": "pnpm run test --coverage",
    "test:watch": "pnpm run test --watch",
    "generate:ethers-types": "typechain --target=ethers-v6 'abis/**/*.json' --out-dir src/types/contracts",
    "xcode": "xed -b ios",
    "doctor": "npx expo-doctor@latest",
    "eas-build-pre-install": "./scripts/git-lfs-pre-install.sh"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "requireCleanWorkingDir": false,
      "requireBranch": "main"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false,
      "skipChecks": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
